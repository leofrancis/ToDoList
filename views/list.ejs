
<%- include("header") -%>


  <div class="box" id="heading">
    <h1>It's a <%= listTitle %>!</h1>
  </div>

  <div class="box">
    <% for (var i = 0; i < newItemsList.length; i++){  %>
    <div class="item">
      <input type="checkbox">
      <p><%= newItemsList[i] %> </p>
    </div>
    <% } %>

    <form class="item" action="/" method="post">
      <input type="text" name="itemValue" autocomplete="off"  placeholder="New Item" value="">
      <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>

  </div>

<%- include("footer") -%>
